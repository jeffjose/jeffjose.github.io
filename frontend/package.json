{
  "name": "personal-website",
  "description": "personal-website",
  "version": "0.0.1",
  "scripts": {
    "dev": "sapper dev",
    "build": "sapper build --legacy",
    "export": "sapper export --legacy",
    "start": "node __sapper__/build",
    "cy:run": "cypress run",
    "cy:open": "cypress open",
    "test": "run-p --race dev cy:run",
    "py": "cd __sapper__/export; python3 -m http.server",
    "docker": "docker build --tag=gcr.io/personal-c913b/personal-website .; docker run -e 'PORT=3000' -p 80:3000 gcr.io/personal-c913b/personal-website",
    "dockerrun": "docker run -e 'PORT=3000' -p 80:3000 gcr.io/personal-c913b/personal-website",
    "dockerinspect": "docker build --tag=gcr.io/personal-c913b/personal-website .; docker run -e 'PORT=3000' -p 80:3000 -t -i gcr.io/personal-c913b/personal-website /bin/bash",
    "cloud": "docker build --tag=gcr.io/personal-c913b/personal-website .; docker push gcr.io/personal-c913b/personal-website",
    "deployclouddev": "gcloud beta run deploy personal-website --platform managed --image gcr.io/personal-c913b/personal-website --region us-central1",
    "cloudprod": "docker build --tag=gcr.io/personal-c913b/personal-website-prod .; docker push gcr.io/personal-c913b/personal-website-prod",
    "deploycloudprod": "gcloud beta run deploy personal-website-prod --platform managed --image gcr.io/personal-c913b/personal-website-prod --region us-central1"
  },
  "dependencies": {
    "compression": "^1.7.1",
    "polka": "next",
    "sirv": "^0.4.0"
  },
  "devDependencies": {
    "@babel/core": "^7.0.0",
    "@babel/plugin-syntax-dynamic-import": "^7.0.0",
    "@babel/plugin-transform-runtime": "^7.0.0",
    "@babel/preset-env": "^7.0.0",
    "@babel/runtime": "^7.0.0",
    "@rollup/plugin-replace": "^2.2.0",
    "asciidoctor": "^2.0.3",
    "asciidoctor-highlight.js": "^0.2.0",
    "npm-run-all": "^4.1.5",
    "pug": "^2.0.4",
    "reading-time": "^1.2.0",
    "rollup": "^1.12.0",
    "rollup-plugin-babel": "^4.0.2",
    "rollup-plugin-commonjs": "^10.0.0",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-svelte": "^5.0.1",
    "rollup-plugin-terser": "^4.0.4",
    "sapper": "^0.27.0",
    "sass": "^1.24.0",
    "svelte": "^3.0.0",
    "svelte-preprocess": "^3.3.0"
  }
}
