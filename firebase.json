{
  "hosting": {
    "public": "dist",
    "trailingSlash": false,
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "rewrites": [
      {
        "source": "profile.jpg",
        "destination": "/homepage/profile.jpg"
      },
      {
        "source": "**",
        "destination": "/index.html"
      }
    ]
  }
}
