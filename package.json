{
  "name": "personal-website",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve --host 0.0.0.0",
    "build": "vue-cli-service build",
    "buildall": "yarn-recursive --cmd build",
    "installall": "yarn-recursive --cmd install",
    "lint": "vue-cli-service lint",
    "deploy": "yarn buildall && firebase deploy",
    "upgradeall": "yarn-recursive --cmd upgrade"
  },
  "dependencies": {
    "bulma": "^0.7.2",
    "chroma-js": "^2.0.3",
    "gsap": "^2.0.2",
    "lodash": "^4.17.11",
    "vue": "^2.5.22",
    "vue-router": "^3.0.1",
    "vuex": "^3.0.1"
  },
  "resolutions": {
    "terser": "3.14.1"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^3.4.0",
    "@vue/cli-service": "^3.4.0",
    "babel-eslint": "^10.0.1",
    "coffee-loader": "^0.9.0",
    "coffeescript": "^2.3.2",
    "eslint": "^5.8.0",
    "eslint-plugin-vue": "^5.0.0",
    "firebase-tools": "^6.3.1",
    "global": "^4.3.2",
    "node-sass": "^4.11.0",
    "pug": "^2.0.3",
    "pug-plain-loader": "^1.0.0",
    "sass-loader": "^7.1.0",
    "vue-cli-plugin-coffeescript": "^0.0.3",
    "vue-template-compiler": "^2.5.21",
    "yarn-recursive": "^1.0.7"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "rules": {},
    "parserOptions": {
      "parser": "babel-eslint"
    }
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ]
}
