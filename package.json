{
  "name": "personal-website",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve --host 0.0.0.0",
    "build": "vue-cli-service build --no-clean",
    "buildclean": "vue-cli-service build",
    "buildall": "yarn-recursive --cmd build",
    "installall": "yarn-recursive --cmd install",
    "lint": "vue-cli-service lint",
    "deploy": "yarn buildall && firebase deploy",
    "upgradeall": "yarn-recursive --cmd upgrade"
  },
  "dependencies": {
    "@asciidoctor/core": "^2.0.2",
    "asciidoctor": "^2.0.0-rc.2",
    "asciidoctor-highlight.js": "^0.2.0",
    "axios": "^0.19.0",
    "gsap": "^2.1.2",
    "highlight.js": "^9.15.6",
    "lodash": "^4.17.11",
    "reading-time": "^1.1.3",
    "vue": "^2.6.6",
    "vue-router": "^3.0.1",
    "vuex": "^3.1.0"
  },
  "resolutions": {
    "terser": "3.14.1"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^3.5.0",
    "@vue/cli-plugin-eslint": "^3.5.0",
    "@vue/cli-service": "^3.5.0",
    "@vue/eslint-config-prettier": "^4.0.1",
    "babel-eslint": "^10.0.1",
    "coffee-loader": "^0.9.0",
    "coffeescript": "^2.3.2",
    "eslint": "^5.8.0",
    "eslint-plugin-vue": "^5.0.0",
    "fibers": "^3.1.1",
    "pug": "^2.0.3",
    "pug-plain-loader": "^1.0.0",
    "sass": "^1.17.2",
    "sass-loader": "^7.1.0",
    "vue-cli-plugin-coffeescript": "^0.0.3",
    "vue-template-compiler": "^2.5.21"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "rules": {},
    "parserOptions": {
      "parser": "babel-eslint"
    }
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ]
}
